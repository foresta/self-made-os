     1                                  ; haribote-os
     2                                  ; TAB=4
     3                                  ; すぐ終了するだけの簡単なOS
     4                                  
     5                                  	ORG		0xc200		; 0xc200 = 0x8000 + 0x4200. 0x8000 は ブートセクタの先頭がメモリの0x8000にくるようにディスクをメモリに読み込み終わってる
     6                                  
     7 00000000 B013                    	MOV		AL, 0x13	; VGAグラフィックス, 320x200x8bitカラー
     8 00000002 B400                    	MOV		AH, 0x00
     9 00000004 CD10                    	INT		0x10
    10                                  
    11                                  fin:
    12 00000006 F4                      	HLT
    13 00000007 EBFD                    	JMP fin
